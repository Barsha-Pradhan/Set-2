public class boyorgirl {
    public static void check(String s){
        String str="";
        for (int i = 0; i <s.length() ; i++) {
            char ch=s.charAt(i);
            if(str.indexOf(ch)==-1){
                str=str+ch;
            }
        }
        if (str.length()%2==0){
            System.out.println("CHAT WITH HER");
        }
        else{
            System.out.println("IGNORE HIM");
        }
    }
}
